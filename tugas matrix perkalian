import numpy as np
C = np.array([[4, 8], [9, 5], [6, 3]])
D = np.array([[4, 7, 9], [8, 6, 3]])
def matrix_multiplication(C, D):
  if C.shape[1] == D.shape[0]:
    return np.dot(C, D)
  else:
    return "matriks C dan D harus sama"
print("Matriks C:")
print(C)
print(Matriks D:")
print(D)
result_multiplication = matrix_multiplication(C, D)
print("\nHsil perkalian matriks C dan D:")
print(result_multiplication)
